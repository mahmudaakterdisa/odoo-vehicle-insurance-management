<odoo>
    <record id="view_insurance_customer_list" model="ir.ui.view">
        <field name="name">insurance.customer.list</field>
        <field name="model">insurance.customer</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="email"/>
                <field name="phone"/>
                <field name="existing_insurance_policy_type"/>
            </list>
        </field>
    </record>

    <record id="view_insurance_customer_form" model="ir.ui.view">
        <field name="name">insurance.customer.form</field>
        <field name="model">insurance.customer</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="email"/>
                        <field name="phone"/>
                        <field name="address"/>
                        <field name="status"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
